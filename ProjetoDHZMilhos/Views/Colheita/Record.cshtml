﻿@using ProjetoDHZMilhos.Models.Colheita
@model ColheitaModel
<div>
    <h3>Carro</h3>
    <form>
        <div class="form-group">
            <label>Data Colheita</label>
            <input type="datetime" id="nome" class="form-control" asp-for="Data_Colheita" />
        </div>
        <div class="form-group">
            <label>Qualidade</label>
            <input type="text" id="nome" class="form-control" asp-for="Qualidade" />
        </div>
        <div class="form-group">
            <label>Quantidade</label>
            <input type="number" id="nome" class="form-control" asp-for="Quantidade_Colhida" />
        </div>
    </form>
</div>
